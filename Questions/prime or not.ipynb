    "def isprime(n):\n",
    "    for i in range(2,n):\n",
    "        if n%i==0:\n",
    "            print(\"not prime\")\n",
    "            break\n",
    "    else:                                          # important part * else in for range line\n",
    "        print(\"is prime\")\n",
    "n = int(input())     \n",
    "isprime(n)"
   ]
